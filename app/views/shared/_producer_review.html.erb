<div id="review" class="review-top">
  <p class="user-details"> Firstname Lastname</p>
  <div class="marks">
    <i class="fa <%= elem.mark >= 1 ? 'fa-star' : 'fa-star-o' %>" aria-hidden="true"></i>
    <i class="fa <%= elem.mark >= 2 ? "fa-star" : "fa-star-o" %>" aria-hidden="true"></i>
    <i class="fa <%= elem.mark >= 3 ? "fa-star" : "fa-star-o" %>" aria-hidden="true"></i>
    <i class="fa <%= elem.mark >= 4 ? "fa-star" : "fa-star-o" %>" aria-hidden="true"></i>
    <i class="fa <%= elem.mark >= 5 ? "fa-star" : "fa-star-o" %>" aria-hidden="true"></i>
  </div>
</div>
<div class="review-bottom">
  <p class="content"><%= elem.content %></p>
<hr>
</div>

